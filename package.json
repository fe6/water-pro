  "scripts": {
    "start": "yarn predev && yarn routes && vite serve site",
    "demo": "vite serve demo",
    "routes": "node site/scripts/genrateRoutes.js && node site/scripts/genrateUtilsRoutes.js",
    "site": "yarn routes && node --max_old_space_size=8192  ./node_modules/vite/bin/vite.js build site --base=http://water-v4.chjgo.com/",
    "test": "cross-env NODE_ENV=test WORKFLOW=true jest --config .jest.js",
    "test:dev": "cross-env NODE_ENV=test jest --config .jest.js",
    "lint": "pretty-quick --staged",
    "predev": "node node_modules/esbuild/install.js",
    "compile": "node antd-tools/cli/run.js compile",
    "generator-webtypes": "tsc -p antd-tools/generator-types/tsconfig.json && node antd-tools/generator-types/index.js",
    "pub": "node --max_old_space_size=8192 antd-tools/cli/run.js pub",
    "pre-publish": "node ./scripts/prepub && npm run generator-webtypes",
    "prettier": "prettier -c --write **/*",
    "pretty-quick": "pretty-quick",
    "release": "node --experimental-json-modules ./scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
